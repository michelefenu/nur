  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6e7f5c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#98bc6d"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.infinity",
        "constant.language.nan"
      ],
      "settings": {
        "foreground": "#1ca4c7"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#e89b7e"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#e89b7e"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language"
      ],
      "settings": {
        "foreground": "#e19c3a"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof"
      ],
      "settings": {
        "foreground": "#6fa3d2"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#6fa3d2"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#cbae82"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#fc7261"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.other.inherited-class",
        "meta.class"
      ],
      "settings": {
        "foreground": "#8ebdb6"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#8ebdb6"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#e8c547"
      }
    },
    {
      "name": "Property",
      "scope": [
        "support.type.property-name",
        "entity.name.tag.css",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#e19c3a"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#8ebdb6"
      }
    },
    {
      "name": "CSS Class and ID",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#fc7261"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.font-name.css",
        "support.constant.media-type.css",
        "support.constant.media.css",
        "constant.other.color.rgb-value.css",
        "constant.other.rgb-value.css",
        "support.constant.color.css"
      ],
      "settings": {
        "foreground": "#e89b7e"
      }
    },
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#1ca4c7"
      }
    },
    {
      "name": "JavaScript/JSX React Components",
      "scope": [
        "support.class.component.jsx",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#8ebdb6"
      }
    },
    {
      "name": "JavaScript Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#98bc6d"
      }
    },
    {
      "name": "JavaScript/TypeScript Types",
      "scope": [
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "support.type.primitive.tsx",
        "support.type.builtin.tsx"
      ],
      "settings": {
        "foreground": "#8ebdb6"
      }
    },
    {
      "name": "This",
      "scope": [
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#8ebdb6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#6fa3d2"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#e19c3a"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#98bc6d"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "markup.heading entity.name",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#fc7261",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#1ca4c7"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#e19c3a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#e2dbc4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#98bc6d"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6e7f5c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#fc7261",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Diff Header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#1ca4c7"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#98bc6d"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#fc7261"
      }
    },
    {
      "name": "Diff Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#e19c3a"
      }
    },
    {
      "name": "HTML/XML DOCTYPE",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype entity",
        "meta.tag.sgml.doctype string",
        "meta.tag.metadata.doctype entity.name.tag",
        "meta.tag.metadata.doctype entity.other.attribute-name",
        "meta.tag.metadata.doctype punctuation",
        "meta.tag.metadata.doctype string"
      ],
      "settings": {
        "foreground": "#6e7f5c"
      }
    },
    {
      "name": "HTML/JSX Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#e8c547"
      }
    },
    {
      "name": "Vue Template",
      "scope": [
        "text.html.vue-html",
        "text.html.vue"
      ],
      "settings": {
        "foreground": "#e2dbc4"
      }
    },
    {
      "name": "Vue Directives",
      "scope": [
        "entity.other.attribute-name.directive.vue",
        "punctuation.separator.key-value.vue"
      ],
      "settings": {
        "foreground": "#fc7261"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": "#fc7261",
    "variable": "#e19c3a",
    "variable.readonly": "#e89b7e",
    "variable.defaultLibrary": "#e89b7e",
    "parameter": "#e19c3a",
    "property": "#e19c3a",
    "property.readonly": "#e89b7e",
    "enumMember": "#e89b7e",
    "event": "#fc7261",
    "function.defaultLibrary": "#fc7261",
    "method": "#fc7261",
    "macro": "#fc7261",
    "label": "#e19c3a",
    "comment": "#6e7f5c",
    "string": "#98bc6d",
    "keyword": "#6fa3d2",
    "number": "#1ca4c7",
    "regexp": "#6fa3d2",
    "operator": "#cbae82",
    "namespace": "#8ebdb6",
    "type": "#8ebdb6",
    "struct": "#8ebdb6",
    "class": "#8ebdb6",
    "interface": "#8ebdb6",
    "enum": "#8ebdb6",
    "typeParameter": "#8ebdb6",
    "decorator": "#e8c547"
  }
}
